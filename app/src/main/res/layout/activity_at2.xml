<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textViewPageId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/page_id" />

        <TextView
            android:id="@+id/textViewStudentData"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textViewPageId"
            android:text="@string/student_data" />

        <Button
            android:id="@+id/buttonCloseAT2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/textViewStudentData"
            android:text="@string/close_at2" />

    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>